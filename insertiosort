package misClases;

import java.util.Arrays;

public class Main{
  
    public static void (String[]args){
    int cont=0;
     int a[];
     a[cont]=valor;
     cont++;
     int aux;
     int entrada, salidad;
     for (int i=0;i<cont; i++){
          system.out.println(a[i] + " ");
          System.out.println(" ");
     } // end for
     for (salida=1; salida<cont;salidad++){
         aux=a[salida];
         entrada=salidad;
         while (entrada>0; && a[entrada-1] >= aux){
             a[entrada] = a[entrada - 1];
             --entrada;
         } //end while
         a[entrada] = aux;
         } //end for
      for (int i=0;i<cont; i++){
          system.out.println(a[i] + " ");
          System.out.println(" ");
      } //end for    
     
     
     
        }
          
}
   
